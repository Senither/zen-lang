--FILE--
if (true) {
    println("Inside if-statement");
} elseif (false) {
    println("You don't see this");
} else {
    println("You don't see this");
}

if (false) {
    println("You don't see this");
} elseif (true) {
    println("Inside elseif-statement");
} else {
