--TEST--
Can skip to next iteration of while loop with continue
--FILE--
var num = 0;

while (num < 100) {
    num++;

    if (num % 5 != 0) {
        continue;
    }

    println("Comparing " + string(num) + " % 5 != 0");
}

println("Done");
