--TEST--
Can parse weirdly formatted JSON strings
--FILE--
var obj = json.parse('      {   "string"  :

 "hello world" , "number":  1            ,
    "float"  :3.14159, "boolean":
 true , "null"  : null,"array":[1,
 2,   3]

}')

println(obj.string)
println(obj.number)
println(obj.float)
println(obj.boolean)
println(obj.array)
println(obj['null'])
--EXPECT--
hello world
1
3.141590
true
[1, 2, 3]
null
