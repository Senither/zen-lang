--TEST--
Can merge multiple maps
--FILE--
var map = maps.merge(
  { 1: "one" },
  { 2: "two" },
  { 3: "three" },
  { 4: "four" },
  { 5: "five" },
  { 6: "six" },
  { 7: "seven" },
  { 8: "eight" },
  { 9: "nine" }
)

var result = []
maps.each(map, func (key, value) {
  arrays.push(result, "Key: " + key + ", Value: " + string(value))
})

println(strings.join(arrays.sort(result), "\n"))
--EXPECT--
Key: 1, Value: one
Key: 2, Value: two
Key: 3, Value: three
Key: 4, Value: four
Key: 5, Value: five
Key: 6, Value: six
Key: 7, Value: seven
Key: 8, Value: eight
Key: 9, Value: nine
