--TEST--
Can get values using indexing
--FILE--
var obj = {
    "string": "string key",
    123: "integer key",
    3.14: "float key",
    true: "boolean key",
    "inner": {
        "someKey": "another inner value",
    },
    "arr": [10, 20, 30],
};

println(obj["string"])
println(obj[123])
println(obj[3.14])
println(obj[true])
println(obj['inner']['someKey'])
println(obj['arr'][0])
println(obj['arr'][1])
println(obj['arr'][2])
--EXPECT--
string key
integer key
float key
boolean key
another inner value
10
20
30
